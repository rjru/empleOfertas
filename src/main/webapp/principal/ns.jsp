	function cargarMenu() {
		var randomNo = Math.floor(Math.random()*9999999);
		var url = appContextPath() + '/obtenerMenuHeader?r=' + randomNo;
		jQuery.get(url)
			.success(
				function(data) {
					$("div#menuDerechoHeader").html(data);
				}
			);
	}

	function cerrarMenuAcciones() {
		$('.desplegablesAcciones').css('display','none');
	}

	function buscarAvisos(url) {
		if ($("#palabrasClaveHeader").val() != "") {
			var palabraClave = $.parseHTML($("#palabrasClaveHeader").val());
			palabraClave = $(palabraClave).text();
			window.location = url + "AL"
					+ escape(palabraClave.toLowerCase());
		} else {
			window.location = url;
		}
		return false;
	}
	

	function searchboxScrollBehaviorOnOff(offset){
	    $('#searchBoxHeader').scrollFix({fixOffset:offset, unfixOffset:offset});
	}
	
	function forwardedLogin(fwd) {
		var url = "/popupLogin";
		if(fwd!=undefined){
			url = url+"?fwd="+fwd;
		}
		showModalByUrl(appContextPath() + url,"");
		$('#seguirConectadoChk').prop('checked', true);
	}
	
</script>
	
